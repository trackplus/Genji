<?xml version="1.0" encoding="UTF-8"?>
<!-- 
	Description file for a Track+ report template. This file
	is read by Track+ and used for presentation of the report
	template for selection by the users. 
-->
<reportTemplate>
  <name>RTDetailedLS</name>	
  <locale value="en">
      <description>
  	    Detailed report with history, graphics, overdue markers, etc.. Demonstrates
  	    quite a number of capabilities of the Track+ reporting engine.
      </description>
      <listing>Change History</listing>
      <tool-tip>Detailed report with history, landscape A4</tool-tip>
  </locale>
  <locale value="de">
      <description>
  	     Detaillierter Bericht mit Verlauf, Grafikelementen, Markierungen für überfällige Vorgänge.
  	     Demonstriert eine Reihe von Möglichkeiten der Track+-Berichtsmaschine.
      </description>  
      <listing>Änderungshistorie</listing>
      <tool-tip>Detaillierter Bericht mit Änderungshistorie A4 Querformat</tool-tip>
  </locale>  
  <!-- The icon should be in GIF format and have a size (width x height) of at most 150 x 100 -->
  <icon-gif>icon.gif</icon-gif>
  <size>A4 landscape</size>
  <!-- The preview should be in GIF format and have a size of at most 800 x 600 -->
  <preview-gif>preview.png</preview-gif>
  <!-- Type of report: Jasper Report or FOP Report -->
  <type>Jasper Report</type>
  <!-- Format of report: pdf, rtf, xml, xsl, png etc -->
  <format>pdf</format>
  <!-- The compiled master report template file -->
  <masterFile>TrackReportWithHistory.jasper</masterFile>
  <xslt>HistoryChange.xsl</xslt>
  <history>true</history>  
</reportTemplate>
