<?xml version="1.0" encoding="UTF-8"?>
<!-- Description file for a Track+ report template. This file is read by 
	Track+ and used for presentation of the report template for selection by 
	the users. -->
<reportTemplate>
	<name>Opened vs. closed chart</name>

	<locale value="en">
	<description>
		Chart with number of items opened vs. closed in a given time interval
	</description>
	<listing>Number of items opened vs. closed</listing>
	<tool-tip>Chart with number of items opened vs. closed, landscape A4</tool-tip>	
	</locale>
	<locale value="de">
	<description>
		Diagramm mit geöffneten und geschlossenen Vorgängen über der Zeit
	</description>
	<listing>Diagramm geschlossene vs. geöffnete Vorgänge</listing>
	<tool-tip>Diagramm geschlossene vs. geöffnete Vorgänge, A4-Querformat</tool-tip>	
	</locale>

	<!-- The icon should be in GIF format and have a size (width x height) of 
		at most 150 x 100 -->
	<icon-gif>icon.gif</icon-gif>
	<size>A4 landscape</size>
	<!-- The preview should be in GIF format and have a size of at most 800 
		x 600 -->
	<preview-gif>preview.png</preview-gif>
	<!-- Type of report: Jasper Report or FOP Report -->
	<type>Jasper Report</type>
	<!-- Format of report: pdf, rtf, xml, xsl, png etc -->
	<format>pdf</format>
	<!-- The compiled master report template file -->
	<masterFile>OpenedVsClosed.jasper</masterFile>
	<datasourcePlugin>com.aurel.track.report.datasource.openedVsClosed.OpenedClosedDatasource</datasourcePlugin>
</reportTemplate>
