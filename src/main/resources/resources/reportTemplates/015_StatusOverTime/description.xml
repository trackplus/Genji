<?xml version="1.0" encoding="UTF-8"?>
<!-- Description file for a Track+ report template. This file is read by 
	Track+ and used for presentation of the report template for selection by 
	the users. -->
<reportTemplate>
	<name>Status over Time Table</name>

	<locale value="en">
		<description>
		Table with number of items in statuses over time
	</description>
	<listing>Table with number of items in statuses over time</listing>
	<tool-tip>Table with number of items in statuses over time, landscape A4</tool-tip>	
	</locale>
	
	<locale value="de">
		<description>
		Tabelle mit Anzahl Vorgänge je Zustand über der Zeit
	</description>
	<listing>Tabelle mit Anzahl Vorgänge je Zustand über der Zeit</listing>
	<tool-tip>Tabelle mit Anzahl Vorgänge je Zustand über der Zeit, A4-Querformat</tool-tip>	
	</locale>

	<!-- The icon should be in GIF format and have a size (width x height) of 
		at most 150 x 100 -->
	<icon-gif>icon.gif</icon-gif>
	<size>A4 landscape</size>
	<!-- The preview should be in GIF format and have a size of at most 800 
		x 600 -->
	<preview-gif>preview.gif</preview-gif>
	<!-- Type of report: Jasper Report or FOP Report -->
	<type>Jasper Report</type>
	<!-- Format of report: pdf, rtf, xml, xls, png etc -->
	<format>xls</format>
	<!-- The compiled master report template file -->
	<masterFile>StatusOverTime.jasper</masterFile>
	<datasourcePlugin>com.aurel.track.report.datasource.statusOverTime.StatusOverTimeDatasource</datasourcePlugin>
</reportTemplate>
