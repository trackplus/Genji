<?xml version="1.0" encoding="UTF-8"?>
<!-- 
	Description file for a Track+ report template. This file
	is read by Track+ and used for presentation of the report
	template for selection by the users. 
-->
<reportTemplate>
  <name>RTByResponsible</name>	
  <locale value="en">
     <description>
  	   Overview report, grouped by responsible.
     </description>
     <listing>Grouped by Responsible</listing>
     <tool-tip>Report overview grouped by responsible, no history, landscape A4</tool-tip>
  </locale>
  <locale value="de">
     <description>
  	   Bericht gruppiert nach Bearbeiter.
     </description>
     <listing>Gruppiert nach Bearbeiter</listing>
     <tool-tip>Ãœbersichtsbericht gruppiert nach Bearbeiter ohne Verlauf, A4-Querformat</tool-tip>
  </locale>
  <!-- The icon should be in GIF format and have a size (width x height) of at most 150 x 100 -->
  <icon-gif>icon.gif</icon-gif>
  <size>A4 landscape</size>
  <!-- The preview should be in GIF or PNG format and have a size of at most 800 x 600 -->
  <preview-gif>preview.png</preview-gif>
  <!-- Type of report: Jasper Report or FOP Report -->
  <type>Jasper Report</type>
  <!-- Format of report: pdf, rtf, xml, xsl, png etc -->
  <format>pdf</format>
  <!-- The compiled master report template file -->
  <masterFile>TrackReportByResponsible.jasper</masterFile>
</reportTemplate>
