<?xml version="1.0" encoding="UTF-8"?>
<!-- Description file for a Track+ report template. This file is read by 
	Track+ and used for presentation of the report template for selection by 
	the users. -->
<reportTemplate>
	<name>Filtered history</name>

	<locale value="en">
	<description>
		Detailed report with filtered history
	</description>
	<listing>Detailed report with filtered history</listing>
	<tool-tip>Detailed report with history, landscape A4</tool-tip>	
	</locale>

	<locale value="de">
	<description>
		Bericht mit Änderungsverlaufsfilter
	</description>
	<listing>Bericht mit Änderungsverlaufsfilter</listing>
	<tool-tip>Bericht mit Änderungsverlaufsfilter, A4-Queerformat</tool-tip>	
	</locale>
	<!-- The icon should be in GIF format and have a size (width x height) of 
		at most 150 x 100 -->
	<icon-gif>icon.gif</icon-gif>
	<size>A4 landscape</size>
	<!-- The preview should be in GIF format and have a size of at most 800 
		x 600 -->
	<preview-gif>preview.png</preview-gif>
	<!-- Type of report: Jasper Report or FOP Report -->
	<type>Jasper Report</type>
	<!-- Format of report: pdf, rtf, xml, xsl, png etc -->
	<format>pdf</format>
	<!-- The compiled master report template file -->
	<masterFile>TrackReportWithHistory.jasper</masterFile>
	<xslt>HistoryChange.xsl</xslt>
	<datasourcePlugin>com.aurel.track.report.datasource.filterHistory.FilterHistoryDatasource</datasourcePlugin>

</reportTemplate>
